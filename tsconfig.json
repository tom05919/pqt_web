{
    "compilerOptions": {
      "target": "esnext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "incremental": true,
      "jsx": "preserve",
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true
    }
